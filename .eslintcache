,"import React, { useState } from \"react\";\r\nimport styles from \"./CartItem.module.css\";\r\n\r\nimport { connect } from \"react-redux\";\r\nimport {\r\n  adjustItemQty,\r\n  removeFromCart,\r\n} from \"../../../redux/Shopping/shopping-action\";\r\n\r\nconst CartItem = ({ item, adjustQty, removeFromCart }) => {\r\n  const [input, setInput] = useState(item.qty);\r\n  \r\n  // adjust quantity of item in cart\r\n  const onChangeHandler = (e) => {\r\n    setInput(e.target.value);\r\n    adjustQty(item.id, e.target.value);\r\n  };\r\n\r\n  return (\r\n    <div className={styles.cartItem}>\r\n      <img\r\n        className={styles.cartItem__image}\r\n        src={'https://flymaxindia.stnshops.com'+item.images[0]}\r\n        alt=\"Product image\"\r\n      />\r\n      {/* title and price of product */}\r\n\r\n      <div className={styles.cartItem__details}>\r\n        <p className={styles.details__title}>{item.title}</p>\r\n        <p className={styles.details__price}>Rs {item.price}</p>\r\n      </div>\r\n\r\n      {/* item quantity adjust by onChangeHandler */}\r\n      <div className={styles.cartItem__actions}>\r\n        <div className={styles.cartItem__qty}>\r\n          <label htmlFor=\"qty\">Qty</label>\r\n          <input\r\n            min=\"1\"\r\n            type=\"number\"\r\n            id=\"qty\"\r\n            name=\"qty\"\r\n            value={input}\r\n            onChange={onChangeHandler}\r\n          />\r\n        </div>\r\n        {/* call to remove from cart */}\r\n        <button\r\n          onClick={() => removeFromCart(item.id)}\r\n          className={styles.actions__deleteItemBtn}\r\n           > \r\n          Delete\r\n        </button>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nconst mapDispatchToProps = (dispatch) => {\r\n  return {\r\n    adjustQty: (id, value) => dispatch(adjustItemQty(id, value)),\r\n    removeFromCart: (id) => dispatch(removeFromCart(id)),\r\n  };\r\n};\r\n\r\nexport default connect(null, mapDispatchToProps)(CartItem);","C:\\Users\\DHANASHRI\\Desktop\\shop\\eshopping\\src\\redux\\Shopping\\shopping-action.js",[],"C:\\Users\\DHANASHRI\\Desktop\\shop\\eshopping\\src\\redux\\Shopping\\shopping-types.js",[],"C:\\Users\\DHANASHRI\\Desktop\\shop\\eshopping\\src\\redux\\Shopping\\shopping-reducer.js",[],{"ruleId":"62","replacedBy":"63"},{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","severity":1,"message":"67","line":10,"column":7,"nodeType":"68","endLine":14,"endColumn":9},{"ruleId":"66","severity":1,"message":"67","line":14,"column":7,"nodeType":"68","endLine":18,"endColumn":9},{"ruleId":"66","severity":1,"message":"67","line":21,"column":7,"nodeType":"68","endLine":25,"endColumn":9},"no-native-reassign",["69"],"no-negated-in-lhs",["70"],"jsx-a11y/img-redundant-alt","Redundant alt attribute. Screen-readers already announce `img` tags as an image. You donâ€™t need to use the words `image`, `photo,` or `picture` (or any specified custom words) in the alt prop.","JSXOpeningElement","no-global-assign","no-unsafe-negation"]